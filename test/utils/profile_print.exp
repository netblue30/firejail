set timeout 10
spawn $env(SHELL)
match_max 100000


send -- "firejail --name=ftest\r"
expect {
	timeout {puts "TESTING ERROR 0\n";exit}
	"Child process initialized"
}
after 100

spawn $env(SHELL)
send -- "firejail --profile.print=ftest\r"
expect {
	timeout {puts "TESTING ERROR 1\n";exit}
	"/etc/firejail/default.profile"
}


after 100
puts "all done\n"
