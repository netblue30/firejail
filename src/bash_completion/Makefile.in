.PHONY: all
all: firejail.bash_completion

include ../common.mk

firejail.bash_completion: firejail.bash_completion.in
	gawk -f ../man/preproc.awk -- $(MANFLAGS) < $< > $@.tmp
	sed "s|@SYSCONFDIR@|$(sysconfdir)|" < $@.tmp > $@
	rm $@.tmp

.PHONY: clean
clean:
	rm -fr firejail.bash_completion

.PHONY: distclean
distclean: clean
	rm -fr Makefile
