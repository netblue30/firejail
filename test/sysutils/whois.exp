set timeout 10
spawn $env(SHELL)
match_max 100000

send -- "firejail whois debian.org\r"
expect {
	timeout {puts "TESTING ERROR 0\n";exit}
	"Domain Name" {puts "testing ok\n"}
	"Connection refused" {puts "TESTING SKIP: connection refused\n"}
}

after 100
puts "\nall done\n"
