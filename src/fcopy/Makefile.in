all: fcopy

include ../common.mk

%.o : %.c $(H_FILE_LIST) ../include/common.h ../include/syscall.h
	$(CC) $(CFLAGS) $(EXTRA_CFLAGS) $(INCLUDE) -c $< -o $@

fcopy: $(OBJS)
	$(CC)  $(LDFLAGS) -o $@ $(OBJS) $(LIBS) $(EXTRA_LDFLAGS)

clean:; rm -f *.o fcopy *.gcov *.gcda *.gcno

distclean: clean
	rm -fr Makefile
