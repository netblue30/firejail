#!/usr/bin/expect -f

set timeout 10
spawn $env(SHELL)
match_max 100000

send -- "firejail --bind=/tmp/chroot,mntpoint\r"
expect {
	timeout {puts "TESTING ERROR 0\n";exit}
	"Child process initialized"
}
sleep 1

send -- "ls mntpoint;pwd\r"
expect {
	timeout {puts "TESTING ERROR 1\n";exit}
	"root"
}
sleep 1

puts "\n"

