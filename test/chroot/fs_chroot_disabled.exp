set timeout 10
spawn $env(SHELL)
match_max 100000

send -- "firejail  --chroot=/tmp/chroot\r"
expect {
	timeout {puts "TESTING ERROR 0\n";exit}
	"feature is disabled"
}
sleep 1

puts "all done\n"
