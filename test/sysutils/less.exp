set timeout 10
spawn $env(SHELL)
match_max 100000

send -- "rm -f /tmp/tt\r"
after 500

send -- "firejail less sysutils.sh > /tmp/t\r"
sleep 1

send -- "cat /tmp/t | grep Authors\r"
expect {
	timeout {puts "TESTING ERROR 0\n";exit}
	"Firejail Authors"
}
after 100

send -- "rm -f /tmp/t\r"
after 500
puts "\nall done\n"
