set timeout 10
spawn $env(SHELL)
match_max 100000

send --  "firejail --name=test1\r"
expect {
	timeout {puts "TESTING ERROR 0\n";exit}
	"Child process initialized"
}
sleep 1

spawn $env(SHELL)
send --  "firejail --name=test2\r"
expect {
	timeout {puts "TESTING ERROR 1\n";exit}
	"Child process initialized"
}
sleep 1

spawn $env(SHELL)
send --  "firejail --top\r"
sleep 4
expect {
	timeout {puts "TESTING ERROR 2\n";exit}
	"name=test1"
}
expect {
	timeout {puts "TESTING ERROR 2\n";exit}
	"name=test2"
}

after 100

puts "\nall done\n"
