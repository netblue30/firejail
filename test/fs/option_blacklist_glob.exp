set timeout 10
spawn $env(SHELL)
match_max 100000

send -- "firejail --blacklist=testdir1/*\r"
expect {
	timeout {puts "TESTING ERROR 0\n";exit}
	"Child process initialized"
}
sleep 1
send -- "cd testdir1\r"
sleep 1

send -- "cat .file\r"
expect {
	timeout {puts "TESTING ERROR 1\n";exit}
	"Permission denied"
}

send -- "ls .directory\r"
expect {
	timeout {puts "TESTING ERROR 2\n";exit}
	"Permission denied"
}
after 100

puts "\n"
