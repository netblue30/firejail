set timeout 10
spawn $env(SHELL)
match_max 100000

# check eth0
send -- "firejail --net=br0 --net=br1 --ip=10.100.10.47\r"
expect {
	timeout {puts "TESTING ERROR 0.0\n";exit}
	"the IP address is not"
}
after 100

puts "\n"
