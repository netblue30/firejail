set timeout 10
spawn $env(SHELL)
match_max 100000

send -- "firejail less sysutils.sh\r"
expect {
	timeout {puts "TESTING ERROR 1\n";exit}
	"MALLOC_CHECK"
}
expect {
	timeout {puts "TESTING ERROR 2\n";exit}
	"./cpio.exp"
}

puts "\nall done\n"
