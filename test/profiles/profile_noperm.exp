set timeout 10
spawn $env(SHELL)
match_max 100000

send -- "firejail --profile=/etc/shadow\r"
expect {
	timeout {puts "TESTING ERROR 0\n";exit}
	"inaccessible profile file"
}
after 100
puts "\nall done\n"
