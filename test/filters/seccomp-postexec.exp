set timeout 10
spawn $env(SHELL)
match_max 100000

send --  "firejail --debug --seccomp=execve\r"
expect {
	timeout {puts "TESTING ERROR 0\n";exit}
	"configuring postexec seccomp filter in"
}
expect {
	timeout {puts "TESTING ERROR 1\n";exit}
	"monitoring pid"
}
sleep 1

send -- "ls\r"
expect {
	timeout {puts "TESTING ERROR 2\n";exit}
	"not permitted"
}


send -- "exit\r"
after 100
puts "all done\n"
