set timeout 10
spawn $env(SHELL)
match_max 100000

send -- "firejail --noprofile --blacklist=\"bla&&bla\"\r"
expect {
	timeout {puts "TESTING ERROR 1.2\n";exit}
	"Error:"
}
expect {
	timeout {puts "TESTING ERROR 1.3\n";exit}
	"is an invalid filename"
}
after 100

